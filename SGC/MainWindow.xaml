            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Width="160" VerticalAlignment="Top" Margin="0,15" Background="Transparent">

                <Image gif:ImageBehavior.AnimatedSource="/Imagenes/unnamed.gif" Margin="0,0,5,0" Visibility="Collapsed" x:Name="pictureBox" ></Image>
                <StackPanel Orientation="Horizontal" Margin="10,0,11,0">
                    <Image x:Name="unlock"  Source="/iconos/unlock.png" Width="25"></Image>
                    <Border  HorizontalAlignment="Right" ToolTip="Editar/no editar el mapa"  Background="#e2e5ee" x:Name="lockk" CornerRadius="12" Width="37"  Height="25" MouseLeftButtonDown="lock_MouseLeftButtonDown" BorderBrush="Transparent" BorderThickness="2">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Image Source="/iconos/lock.png" Width="17" Grid.Column="1" Visibility="Collapsed"></Image>
                            <Image Source="/iconos/unlock.png" Width="17" Grid.Column="0" Visibility="Collapsed"></Image>
                            <Border Grid.ColumnSpan="2" x:Name="bdr3"  Width="9" Height="9" CornerRadius="10" Background="#333333" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3,0"></Border>

                        </Grid>

                    </Border>
                    <Image x:Name="lockkk"  Source="/iconos/lockgray.png" Width="25"></Image>
                </StackPanel>
                <Button Visibility="Collapsed"  Margin="0,0,20,0" ToolTip="Bloquear pantalla"  Background="LightBlue" x:Name="lockb" HorizontalAlignment="Right" Width="40" Height="30" Click="Button_Click">
                    <Button.Resources>
                        <Style TargetType="Border">
                            <Setter Property="CornerRadius" Value="10"></Setter>
                        </Style>
                    </Button.Resources>
                    <Image Source="/iconos/lock.png" Width="30" ></Image>
                </Button>

                <Button Margin="0,0,20,0" ToolTip="Salir de sesión" BorderBrush="Transparent" Background="#e3e6ed" x:Name="logout" HorizontalAlignment="Right" Width="{Binding Source={x:Static properties:Settings.Default}, Path=Botonwidht}" Height="{Binding Source={x:Static properties:Settings.Default}, Path=Botonheight}" Click="Button_Click2">
                    <Button.Resources>
                        <Style TargetType="Border">
                            <Setter Property="CornerRadius" Value="5"></Setter>
                        </Style>
                    </Button.Resources>
                    <Image Source="https://upload.wikimedia.org/wikipedia/commons/thumb/8/8a/OOjs_UI_icon_logOut-ltr.svg/1024px-OOjs_UI_icon_logOut-ltr.svg.png" Width="14" ></Image>
                </Button>
            </StackPanel>

        </Grid>

    </Grid>
</Window>
